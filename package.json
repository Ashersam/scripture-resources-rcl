{
  "name": "scripture-resources-rcl",
  "description": "A React Component Library for Rendering Scripture Resources.",
  "homepage": "https://scripture-resources-rcl.netlify.com/",
  "repository": {
    "type": "git",
    "url": "https://github.com/unfoldingWord-box3/scripture-resources-rcl"
  },
  "version": "0.10.16",
  "private": false,
  "license": "MIT",
  "dependencies": {
    "axios": "^0.19.0",
    "deep-freeze": "0.0.1",
    "dog-names": "^2.0.0",
    "gitea-react-toolkit": "^0.5.2",
    "js-yaml-parser": "^1.0.0",
    "lodash": "^4.17.15",
    "prop-types": "^15.7.2",
    "react-json-view": "^1.19.1",
    "react-styleguidist": "^9.1.14",
    "react-waypoint": "^9.0.2",
    "string-punctuation-tokenizer": "^2.0.0-beta.2",
    "usfm-js": "^2.0.2"
  },
  "scripts": {
    "start": "styleguidist server",
    "build": "styleguidist build",
    "deploy": "git push",
    "postpublish": "yarn deploy",
    "prepublishOnly": "rm -fr ./dist & babel ./src --out-dir ./dist -s inline",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "nyc:report": "nyc report --reporter=json-summary --reporter=text",
    "test": "start-test 6060 cypress:run && npm run nyc:report",
    "create-coverage-badge": "bash scripts/create-badge-json.sh"
  },
  "eslintConfig": {
    "extends": "react-app",
    "plugins": [
      "react-hooks"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "peerDependencies": {
    "@material-ui/core": "^4.7.0",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/lab": "^4.0.0-alpha.32",
    "@material-ui/styles": "^4.4.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.7.4",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@cypress/code-coverage": "^1.10.2",
    "@material-ui/core": "^4.7.0",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/lab": "^4.0.0-alpha.32",
    "@material-ui/styles": "^4.4.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-inline-json-import": "^0.3.2",
    "babel-plugin-istanbul": "^5.2.0",
    "cross-env": "^6.0.3",
    "css-loader": "^3.2.0",
    "cypress": "^3.6.1",
    "file-loader": "^4.2.0",
    "istanbul-lib-coverage": "^2.0.5",
    "nyc": "^14.1.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "start-server-and-test": "^1.10.6",
    "style-loader": "^1.0.0",
    "url-loader": "^2.1.0",
    "webpack": "^4.39.1"
  },
  "main": "dist/index.js",
  "files": [
    "dist/*"
  ],
  "nyc": {
    "include": [
      "src/components/**/**.js"
    ],
    "exclude": [
      "**/mocks/**"
    ]
  }
}